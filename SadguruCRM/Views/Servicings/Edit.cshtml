            $.each($('input[id^="ChemicalIDListBox"]'), function (index1, obj1) {
                if ($(obj1).val() == obj.ChemicalID) {
                    //alert($(obj1).val());
                    $(obj1).parent().parent().find("#ChemicalSelectedListBox").attr('checked', 'checked');
                    $(obj1).parent().parent().find("#UOMIDListBox").val(obj.UOMID);
                    $(obj1).parent().parent().find("#ChemicalQtyListBox").val(obj.ChemicalQty);

                }
            });
            //alert(index + ": " + obj.ChemicalID);
        });

        if (('@Model.ServicingNo').indexOf("-") != -1) {
            //alert("comp");
            $(".compTable").hide();
            $("#CreateComplaint").hide();
        }
    });

    $(".openComplaint").click(function () {
        //alert($(this).parent(".singleComplaintTD").find("#ThisCompID").val());
        var thisComplaintElement = $(this).parent(".singleComplaintTD")[0];
        debugger;
        $("#ComplaintID").val($(thisComplaintElement).find("#ThisCompID").val());
        $("#ComplaintNo").val($(thisComplaintElement).find("#ThisCompNo").val());
        $("#ComplaintContent").val($(thisComplaintElement).find("#ThisCompContent").val());
        $("#Complaint_Servicing_Datetime").val($(thisComplaintElement).find("#ThisCompServiceDate").val());
        $("#Complaint_Servicing_Time").val($(thisComplaintElement).find("#ThisCompServiceTime").val());
        $("#Complaint_Actual_Servicing_Datetime").val($(thisComplaintElement).find("#ThisCompActualServiceDate").val());
        $("#Complaint_Actual_Servicing_Time").val($(thisComplaintElement).find("#ThisCompActualServiceTime").val());
        $('#ComplaintTechnicianID').selectpicker('val', JSON.parse($(thisComplaintElement).find("#ThisCompTechnicianIDs").val()) );
        $("#ComplaintTechnicianID").selectpicker("refresh");
        $("#ComplaintVendorID").val($(thisComplaintElement).find("#ThisCompVendorID").val());
        $("#ComplaintVendorID").selectpicker("refresh");
        $('#ComplaintChemicalID').selectpicker('val', JSON.parse($(thisComplaintElement).find("#ThisCompChemicalIDs").val()));
        $("#ComplaintChemicalID").selectpicker("refresh");
        $('#ComplaintChemicalTechnicianID').selectpicker('val', JSON.parse($(thisComplaintElement).find("#ThisCompChemicalTechnicianIDs").val()));
        $("#ComplaintChemicalTechnicianID").selectpicker("refresh");
        $("#ComplaintChemicalQty").val(JSON.parse($(thisComplaintElement).find("#ThisCompChemicalQty").val()));
        $('#ComplaintUOMID').selectpicker('val', JSON.parse($(thisComplaintElement).find("#ThisCompUOMIDs").val()));
        $("#ComplaintUOMID").selectpicker("refresh");
        $("#ComplaintStatusID").val($(thisComplaintElement).find("#ThisCompServiceStatus").val());

        $("#ComplaintStatusID").selectpicker("refresh");
        $("#ComplaintServicingInstructions").val($(thisComplaintElement).find("#ThisCompServiceInstructions").val());
        $('.modal').modal('show');
    });
    $("#CreateComplaint").click(function () {
        $("#ComplaintNo").val($("#ComplaintServicingNo").val());
        $('.modal').modal('show');
        return false;
    });
</script>
