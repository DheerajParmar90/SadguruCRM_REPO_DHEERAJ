    <h5>Issued Chemicals</h5>
    <div class="form-group row">
        <div class="col-8">
            <tablebody>
                <table style="width: 100%; background: white; border: solid; border-color: black; border-width: 2px; height: 150px; overflow-y: auto; display: block; border-collapse: separate; border-spacing: 0 3px;">
                    <tbody>
                        @{
                            var ChemicalsListForCheckBoxList = (SelectList)ViewBag.ChemicalsListForCheckBoxList;
                            for (int i = 0; i < ChemicalsListForCheckBoxList.Count(); i++)
                            {
                                //var chemical = (SadguruCRM.Models.Chemicals_Master)
                                <tr style="padding:3px;">
                                    <td style="padding:3px;width:10%;">
                                        @Html.CheckBox("ChemicalSelectedListBox", ChemicalsListForCheckBoxList.ElementAt(i).Selected, new { @class = "ChemicalSelectedListBox" })
                                    </td>
                                    <td style="padding:3px;width:30%;">
                                        <text>@ChemicalsListForCheckBoxList.ElementAt(i).Text</text>
                                        @Html.Hidden("ChemicalIDListBox", ChemicalsListForCheckBoxList.ElementAt(i).Value)

