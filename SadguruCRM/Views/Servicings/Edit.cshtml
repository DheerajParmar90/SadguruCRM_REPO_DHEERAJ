                                    <td style="width:30%" class="tdForDropdown">
                                        <divfordropdown style="z-index:99;">
                                            @Html.DropDownList("UOMIDListBox", (IEnumerable<SelectListItem>)ViewBag.UOMListForCheckBoxList, "-- SELECT UOM --"
