    <table class="table table-striped compTable">
        <thead>
            <tr>
                <th>
                    Complaint ID
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (SadguruCRM.Models.Servicing complaint in ViewBag.Complaints)
            {
                <tr>
                    <td class="singleComplaintTD">
                        <button class="btn btn-sm btn-outline-secondary openComplaint" type="button">@complaint.ServicingNo</button>
                        @Html.Hidden("ThisCompID", complaint.ServicingID.ToString())
                        @Html.Hidden("ThisCompNo", (string)complaint.ServicingNo)
                        @Html.Hidden("ThisCompContent", (string)complaint.ComplaintContent)
                        @Html.Hidden("ThisCompServiceDate",
                                        complaint.Servicing_Datetime.HasValue ? complaint.Servicing_Datetime.Value.ToString("dd/MM/yyyy") : "")
                        @Html.Hidden("ThisCompServiceTime",
                                        complaint.Servicing_Datetime.HasValue ? complaint.Servicing_Datetime.Value.ToString("hh:mm tt") : "")
                        @Html.Hidden("ThisCompActualServiceDate",
                                        complaint.Actual_Servicing_Datetime.HasValue ? complaint.Actual_Servicing_Datetime.Value.ToString("dd/MM/yyyy") : "")
                        @Html.Hidden("ThisCompActualServiceTime",
                                        complaint.Actual_Servicing_Datetime.HasValue ? complaint.Actual_Servicing_Datetime.Value.ToString("hh:mm tt") : "")
                        @Html.Hidden("ThisCompTechnicianIDs", Json.Encode(complaint.Servicing_Technician_Mapping.Select(map => map.TechnicianID)))
                        @Html.Hidden("ThisCompVendorID", complaint.VendorID)
                        @Html.Hidden("ThisCompChemicalIDs", Json.Encode(complaint.Servicing_Chemicals_Issued.Select(c => c.ChemicalID).FirstOrDefault()))
                        @Html.Hidden("ThisCompChemicalQty", Json.Encode(complaint.Servicing_Chemicals_Issued.Select(c => c.ChemicalQty).FirstOrDefault()))
                        @Html.Hidden("ThisCompChemicalTechnicianIDs", Json.Encode(complaint.Servicing_Chemicals_Issued.Select(map => map.TechnicianID).FirstOrDefault()))
                        @Html.Hidden("ThisCompUOMIDs", Json.Encode(complaint.Servicing_Chemicals_Issued.Select(map => map.UOMID).FirstOrDefault()))
                        @Html.Hidden("ThisCompServiceStatus", complaint.Service_Status_ID)
                        @Html.Hidden("ThisCompServiceInstructions", complaint.ServicingInstructions)
                    </td>
                </tr>
            }
        </tbody>
    </table>
    <div class="form-group row" style="text-align:right">

        <button id="CreateComplaint" class="btn btn-sm btn-secondary" type="button" style="float:right;">Create Complaint</button>
    </div>
    <div class="form-group">
        <div>
            <input type="submit" value="Save" class="btn btn-secondary" style="float:right;" />
        </div>
    </div>
</div>
