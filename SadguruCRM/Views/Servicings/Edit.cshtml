<div class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document" style="max-width:80%;">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Complaint</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            @using (Html.BeginForm("CreateEditComplaint", "Servicings", FormMethod.Post, new { id = "ServicingCreate", enctype = "multipart/form-data" }))
            {

                @Html.AntiForgeryToken()
                <div class="modal-body">

                    @Html.HiddenFor(model => model.ServicingID)
                    @Html.Hidden("ComplaintServicingNo", Model.ServicingNo + "-" + (string)(ViewBag.ServicePlusComplaintsCount))
                    @Html.Hidden("ComplaintID")
                    <div class="form-group row">
                        <div class="col-12">
                            @Html.Label("Complaint ID", "Complaint ID", htmlAttributes: new { @class = "control-label" })
                            @Html.Editor("ComplaintNo", new { htmlAttributes = new { @class = "form-control", @readonly = "@readonly" } })
                            @Html.ValidationMessage("ComplaintNo", "", new { @class = "text-danger" })
                        </div>
                    </div>

                    <div class="form-group row">
                        <div class="col-12">
                            @Html.Label("Complaint", "Complaint", htmlAttributes: new { @class = "control-label" })
                            @Html.TextArea("ComplaintContent", "", 4, 200, new { @class = "form-control" })
                            @Html.ValidationMessage("ComplaintContent", "", new { @class = "text-danger" })
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-3">
                            @Html.Label("Service Date", "Service Date", htmlAttributes: new { @class = "control-label" })
                            @Html.Editor("Complaint_Servicing_Datetime", new { htmlAttributes = new { @class = "form-control", @autocomplete = "off" } })
                        </div>
